<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.project.bean.KindBean" table="Kind" catalog="managementbus">
		<id name="kindNumber" type="java.lang.Integer">
			<column name="kindNumber" />
			<generator class="identity" />
		</id>
		<property name="kind" type="string">
			<column name="kind" length="45" not-null="true" />
		</property>
		<property name="type" type="string">
			<column name="type" length="45" not-null="true" />
		</property>
		<property name="category" type="string">
			<column name="category" length="45" not-null="true" />
		</property>
	</class>
</hibernate-mapping>