<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.webservice.bean.User" table="Users">
		<id name="userNumber" column="UserNumber" type="string">
			<generator class="native" />
		</id>

		<property name="username" column="Username" type="string" />
		<property name="password" column="Password" type="string" />
		<property name="email" column="Email" type="string" />
		<property name="account" column="Account" type="string" />

		<!-- <many-to-one name="busNumber" column="BusNumber" unique="true" class="com.webservice.bean.Bus" 
			not-null="true" /> -->
	</class>
</hibernate-mapping>
